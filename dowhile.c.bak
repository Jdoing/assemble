#include<stdio.h>
int fact_do(int n)
{
    int result = 1;
    do
    {
        result *= n;
        n = n-1;
    }while(n > 1);
    
	return result;
}

int main()
{
    int n = 3;
	int result = fact_do(n);
    printf("result=%d\n",result);
    return 0;
}
